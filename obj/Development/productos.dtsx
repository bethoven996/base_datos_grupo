                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT TOP (1000) [id_producto]
      ,[desc_producto]
      ,[presentacion_producto]
  FROM [starying].[dbo].[sty_productos_2025]


SELECT 
    t.id_producto,
    t.[desc_producto],

   
	Agrupador_Producto = RIGHT(
		TRIM(t.[desc_producto]),
		CHARINDEX(' ', REVERSE(TRIM(t.[desc_producto])) + ' ') - 1
	),
   
    TRY_CAST(parts.[0] AS INT) AS Capacidad,

   
    parts.[1] AS Medidas,

   
    CASE 
        WHEN LOWER(parts.[2]) IN ('can', 'lata') THEN 'Sí'
        ELSE 'No'
    END AS Es_lata,

    CASE
        WHEN LOWER(parts.[1]) IN ('liter', 'litro', 'litros') 
            THEN TRY_CAST(parts.[0] AS FLOAT) * 1000
        WHEN LOWER(parts.[1]) IN ('cm3', 'cc') 
            THEN TRY_CAST(parts.[0] AS FLOAT)
        ELSE NULL
    END AS Capacidad_cm3

FROM [starying].[dbo].[sty_productos_2025] AS t
CROSS APPLY (
    SELECT 
        MAX(CASE WHEN [key] = 0 THEN value END) AS [0],
        MAX(CASE WHEN [key] = 1 THEN value END) AS [1],
        MAX(CASE WHEN [key] = 2 THEN value END) AS [2]
    FROM OPENJSON('["' + REPLACE(ISNULL(t.[presentacion_producto], ''), ' ', '","') + '"]')
) parts;</property>
